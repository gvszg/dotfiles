#!/bin/bash
#Generated by JetBrains Toolbox 1.26.0.13072 at 2022-09-25T11:21:25.642179

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/aeroyu/Library/Application Support/JetBrains/Toolbox/apps/WebStorm/ch-0/222.4167.31/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${ideargs[@]}"